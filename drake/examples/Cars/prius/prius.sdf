      <child>left_tie_rod_arm</child>
      <pose>0 0.494736842 0 0 0 0</pose>
      <axis><xyz>0 0 1</xyz></axis> 
      <limit>
        <lower>-0.401425728</lower>
        <upper>0.5</upper>
